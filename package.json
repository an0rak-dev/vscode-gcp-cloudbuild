{
	"name": "vscode-gcp-cloudbuild",
	"displayName": "VSCode support for GCP CloudBuild",
	"description": "Add GCP's CloudBuild infos & actions to VSCode",
	"keywords": [
		"vscode",
		"visual studio code",
		"gcp",
		"cloudbuild"
	],
	"bugs": {
		"url": "https://github.com/an0rak-dev/vscode-gcp-cloudbuild/issues"
	},
	"license": "Apache-2.0",
	"author": {
		"name": "Sylvain Nieuwlandt",
		"url": "https://an0rak.dev"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/an0rak-dev/vscode-gcp-cloudbuild"
	},
	"version": "1.1.0",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "jest --verbose --no-cache",
		"test:dev": "jest --watch",
		"coverage": "jest --coverage"
	},
	"publisher": "an0rak-dev",
	"contributes": {},
	"icon": "assets/cloudbuild_logo.png",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/cloudbuild.yaml"
	],
	"main": "./out/src/extension.js",
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"@types/jest": "^24.0.18",
		"glob": "^7.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"jest": "^24.9.0",
		"ts-jest": "^24.0.2"
	}
}
