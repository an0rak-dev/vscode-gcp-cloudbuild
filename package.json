{
	"name": "vscode-gcp-cloudbuild",
	"displayName": "vscode-gcp-cloudbuild",
	"description": "Add GCP's CloudBuild infos & actions to VSCode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/cloudbuild.yaml"
	],
	"main": "./out/src/extension.js",
	"contributes": {},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "jest",
		"test-dev": "jest -watch"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"@types/jest": "^24.0.18",
		"glob": "^7.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"jest": "^24.9.0",
		"ts-jest": "^24.0.2"
	}
}
